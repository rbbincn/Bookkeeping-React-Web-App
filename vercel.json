{
  "version": 2,
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public,max-age=31536000,immutable" }
      ]
    },
    {
      "source": "/index.html",
      "headers": [
        { "key": "Cache-Control", "value": "no-cache" }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!assets/|favicon\\.ico|robots\\.txt|manifest\\.json).*)",
      "destination": "/index.html"
    }
  ]
}